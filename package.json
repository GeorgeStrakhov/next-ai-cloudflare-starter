{
	"name": "cloudflare-nextjs-starter",
	"version": "1.0.0",
	"description": "Next.js 15 starter template for Cloudflare Workers with D1, Drizzle ORM, and Better Auth, Replicate, R2, OpenRouter and more.",
	"author": "George GeorgeStrakhov",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/GeorgeStrakhov/next-ai-cloudflare-starter"
	},
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply my-app-db-prod --local",
		"db:studio": "drizzle-kit studio",
		"db:studio:stage": "./scripts/db-studio.sh staging",
		"db:studio:prod": "./scripts/db-studio.sh production",
		"deploy:staging": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env staging",
		"deploy:production": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"db:migrate:staging": "wrangler d1 migrations apply my-app-db-staging --remote --env staging",
		"db:migrate:production": "wrangler d1 migrations apply my-app-db-prod --remote"
	},
	"dependencies": {
		"@ai-sdk/react": "^2.0.109",
		"@mdx-js/loader": "^3.1.1",
		"@mdx-js/react": "^3.1.1",
		"@next/mdx": "15.4.8",
		"@next/third-parties": "^16.0.8",
		"@opennextjs/cloudflare": "^1.14.4",
		"@openrouter/ai-sdk-provider": "^1.5.2",
		"@radix-ui/react-alert-dialog": "^1.1.15",
		"@radix-ui/react-avatar": "^1.1.11",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.8",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.8",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@sentry/nextjs": "^10.30.0",
		"@tabler/icons-react": "^3.35.0",
		"@toon-format/toon": "^2.1.0",
		"@types/mdx": "^2.0.13",
		"ai": "^5.0.108",
		"better-auth": "^1.4.6",
		"better-call": "^1.1.5",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.45.0",
		"import-in-the-middle": "^2.0.0",
		"lucide-react": "^0.556.0",
		"marked": "^16.4.1",
		"next": "15.4.8",
		"next-themes": "^0.4.6",
		"openai": "^6.10.0",
		"posthog-js": "^1.303.1",
		"postmark": "^4.0.5",
		"react": "19.2.1",
		"react-dom": "19.2.1",
		"replicate": "^1.4.0",
		"require-in-the-middle": "^8.0.1",
		"slugify": "^1.6.6",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"uuid": "^13.0.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.948.0",
		"@cloudflare/workers-types": "^4.20251210.0",
		"@eslint/eslintrc": "^3.3.3",
		"@tailwindcss/postcss": "^4.1.17",
		"@types/node": "^20",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"better-sqlite3": "^12.5.0",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9.39.1",
		"eslint-config-next": "15.4.8",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"wrangler": "^4.53.0"
	}
}
