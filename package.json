{
	"name": "cloudflare-nextjs-starter",
	"version": "1.0.0",
	"description": "Next.js 15 starter template for Cloudflare Workers with D1, Drizzle ORM, and Better Auth, Replicate, R2, OpenRouter and more.",
	"author": "George GeorgeStrakhov",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/GeorgeStrakhov/next-ai-cloudflare-starter"
	},
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply my-app-db --local",
		"db:studio": "drizzle-kit studio",
		"db:studio:stage": "./scripts/db-studio.sh staging",
		"db:studio:prod": "./scripts/db-studio.sh production",
		"deploy:staging": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env staging",
		"deploy:production": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"db:migrate:staging": "wrangler d1 migrations apply my-app-db-staging --remote --env staging",
		"db:migrate:production": "wrangler d1 migrations apply my-app-db --remote"
	},
	"dependencies": {
		"@ai-sdk/react": "^2.0.89",
		"@aws-sdk/client-s3": "^3.927.0",
		"@opennextjs/cloudflare": "^1.11.0",
		"@openrouter/ai-sdk-provider": "^1.2.1",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.13",
		"ai": "^5.0.89",
		"better-auth": "^1.3.29",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.6",
		"lucide-react": "^0.552.0",
		"marked": "^16.4.1",
		"next": "15.4.6",
		"next-themes": "^0.4.6",
		"openai": "^6.7.0",
		"postmark": "^4.0.5",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"replicate": "^1.3.1",
		"slugify": "^1.6.6",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"uuid": "^13.0.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251014.0",
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"better-sqlite3": "^12.4.1",
		"drizzle-kit": "^0.31.5",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"wrangler": "^4.44.0"
	}
}